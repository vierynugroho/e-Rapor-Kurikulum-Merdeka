{
    "name": "e_rapor",
    "version": "0.1.0",
    "author": "Viery Nugroho",
    "description": "[skripsi] e-Rapor Kurikulum Merdeka PAUD",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "format": "prettier --write .",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "prepare": "husky",
        "prisma": "npx prisma generate",
        "db:migrate-dev": "npx prisma migrate dev",
        "db:migrate-deploy": "npx prisma migrate deploy",
        "db:push": "npx prisma db push",
        "db:studio": "npx prisma studio",
        "db:seed": "set NODE_OPTIONS=--loader ts-node/esm && ts-node --esm prisma/seeder.ts"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npx lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write",
            "eslint  --fix",
            "git add"
        ],
        "**/*": "prettier --write --ignore-unknown"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.9.1",
        "@prisma/client": "^6.1.0",
        "@tanstack/react-query": "^5.62.11",
        "@tanstack/react-table": "^8.20.6",
        "axios": "^1.7.9",
        "next": "14.2.22",
        "next-themes": "^0.4.4",
        "prisma": "^6.1.0",
        "react": "^18",
        "react-day-picker": "^9.5.0",
        "react-dom": "^18",
        "react-hook-form": "^7.54.2",
        "ts-node": "^10.9.2",
        "zod": "^3.24.1",
        "zustand": "^5.0.2"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "eslint": "^8",
        "eslint-config-next": "14.2.22",
        "husky": "^9.1.7",
        "lint-staged": "^15.3.0",
        "postcss": "^8",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.5.3"
    }
}
